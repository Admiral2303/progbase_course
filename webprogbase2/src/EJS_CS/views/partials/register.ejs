<div class="modal fade" id="myModal1" tabindex="-1" role="dialog">
      <div class="modal-dialog">
        <div class="modal-content">

          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">×</button>
            <h4 class="modal-title" id="myModalLabel">Регистрация</h4>
          </div>

          <div class="modal-body">

           
                <div class="panel panel-primary" >
                <div class="panel-heading"></div>
                    <div class="panel-body">
                        <form data-toggle="validator" role="form" action="/register" method="POST" enctype='multipart/form-data'>
                            <div class="form-group">
                                <label class="control-label" for="name" >Імʼя</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                <input class="form-control" data-error="Введіть імʼя" id="name" name="name"  type="text" required />
                                <div class="help-block with-errors"></div>
                            </div>
                            <div class="form-group">
                                <label class="control-label" for="surname">Прізвище</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                <input class="form-control" data-error="Введіть прізвище" id="surname" name="surname" placeholder="Name"  type="text" required />
                                <div class="help-block with-errors"></div>
                            </div>
                            <div class="form-group">
                                <label for="year">Рік</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>
                                <select class="form-control" id="year" name="year">
                                <%for(let i = 2017; i >=1950; i--){%>
                                    <option><%=i%></option>
                                <% } %>
                                </select>
                            </div>
                            <div class="form-group">
                                <label class="control-label" for="country" >Країна</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-globe"></i></span>
                                <input class="form-control" data-error="Введіть імʼя" id="country" name="country"  type="text" required />
                                <div class="help-block with-errors"></div>
                            </div>
                            <div class="form-group">
                                <label class="control-label" for="city" >Місто</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-globe"></i></span>
                                <input class="form-control" data-error="Введіть імʼя" id="city" name="city"  type="text" required />
                                <div class="help-block with-errors"></div>
                            </div>
                            <div class="form-group">
                                <label class="control-label" for="city" >Університет</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-book"></i></span>
                                <input class="form-control" data-error="Введіть університет" id="university" name="university"  type="text" required />
                                <div class="help-block with-errors"></div>
                            </div>
                            <div class="form-group">
                                <label for="sex">Стать</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                <select class="form-control" id="sex" name="sex">                  
                                    <option>Чоловіча</option>
                                    <option>Жіноча  </option>
                                </select>
                            </div>

                            <div class="form-group">
                                <label for="email" class="control-label">Email</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
                                <input type="email" class="form-control" id="email" name="email" data-error="Введіть правильний email" placeholder="Email" required>
                                <div class="help-block with-errors"></div>
                            </div>
                            <div class="form-group">
                                <label for="login" class="control-label">Логін</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-home"></i></span>
                                <input type="text" class="form-control" id="login" name="login" data-error="Введіть правильний email" placeholder="Email" required>
                                <div class="help-block with-errors"></div>
                            </div>
                            <div class="form-group">
                                <label for="password" class="control-label">Пароль</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                                <input type="password" id="password1" name="password" data-minlength="5" class="form-control" id="inputPassword" data-error="Пароль має містити мінімум 5 символів" placeholder="Password" required>
                                <div class="help-block with-errors"></div>
                            </div>

                            <div class="form-group">
                                <label for="password2" class="control-label">Повторіть пароль</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                                <input type="password" id="password2" name="password2" data-minlength="5" class="form-control" id="inputPassword" data-error="Паролі не співпадають" placeholder="Password" required>
                                <div class="help-block with-errors"></div>
                                
                            </div>
                            
                            <div class="form-group">
                                <label class="control-label" for="information">Інформація про себе</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-cloud"></i></span>
                                <textarea class="form-control" data-error="Please enter BIO field." id="information" name="information" placeholder="Cina Saffary" required=""></textarea>
                                <div class="help-block with-errors"></div>
                            </div>
                            <div class="form-group">
                                <label for="ava">Виберіть аватар</label>
                                <span class="form-group-addon"><i class="glyphicon glyphicon-eye-open"></i></span>
                                <input  type="file" required id="ava" name="ava" />
                            </div>

                            <div class="modal-footer">
                                <button type="button" class="btn btn-default" data-dismiss="modal">Отмена</button>
                                <button id="save" type="submit" class="btn btn-primary">Регистрация</button>
                            </div>
                        </form>
                    </div>   
            </div>
          </div>

          
        </div>
      </div>
    </div>
     