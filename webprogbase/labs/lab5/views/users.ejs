<% include partials/header %>

<% for(let user of users){ %>
    <% if(user.role != "admin"){%>
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-2 col-md-offset-2">
                    <img src= "data:<%=user.mimetype%>>;base64,<%=user.image.toString('base64')%>" alt="Garry Potter" style="max-height: 250px; max-width: 250px;border-radius: 25px;">
                </div>
                <div class="col-md-4 col-md-offset-1">
                    <h3 style="margin-top: 20%; color:black; font-family: 'Century Schoolbook L', bold;font-weight: 600">
                            Імʼя - <%=user.name %><br>
                            Рік народження - <%= user.year %> <br>
                            Email - <%= user.email %><br> 
                    </h3>        
                </div>
                </div>
            </div>
        </div>
    <hr>
    <% } %>
<% } %>



